{
  "name": "litekart",
  "version": "2.0.0",
  "description": "Litekart Store Front",
  "author": "Swadesh Behera",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "start": "cross-env NODE_ENV=production nuxt start",
    "prod": "run-s build copyNetlify",
    "copyNetlify": "node devops/copy-netlify.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "copy": "node devops/copy.js",
    "live": "run-s build copy liveS",
    "liveS": "node devops/live.js",
    "generate": "nuxt generate",
    "build": "nuxt build",
    "analyze": "nuxt build -a"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.8.0",
    "@nuxtjs/font-awesome": "^1.0.4",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/toast": "^3.3.0",
    "cookie-universal-nuxt": "^2.0.19",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "node-sass": "^4.13.0",
    "nuxt": "^2.10.2",
    "sass-loader": "^8.0.0",
    "tailwindcss-animatecss": "^0.2.4",
    "vue-carousel": "^0.18.0",
    "vue-clickaway": "^2.2.2",
    "vue-lazyload": "^1.3.3",
    "vue-plain-pagination": "^0.3.0",
    "vue-slider-component": "^3.0.43"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.3",
    "ssh2shell": "^1.9.3"
  }
}