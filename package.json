{
  "name": "arialshop-tailwind",
  "version": "1.0.0",
  "description": "Arialshop Tailwind Store Front",
  "author": "coolnaveesh",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "start": "cross-env NODE_ENV=production nuxt start",
    "prod": "run-s build copyNetlify",
    "copyNetlify": "node devops/copy-netlify.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "copy": "node devops/copy.js",
    "live": "run-s build copy liveS",
    "liveS": "node devops/live.js",
    "generate": "nuxt generate",
    "build": "nuxt build",
    "analyze": "nuxt build -a"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/font-awesome": "^1.0.4",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/robots": "^2.4.0",
    "@nuxtjs/sitemap": "^1.2.0",
    "@nuxtjs/toast": "^3.2.1",
    "cookie-universal-nuxt": "^2.0.17",
    "cross-env": "^5.2.1",
    "dotenv": "^8.1.0",
    "node-sass": "^4.11.0",
    "nuxt": "^2.9.2",
    "sass-loader": "^7.1.0",
    "vue-carousel": "^0.18.0",
    "vue-clickaway": "^2.2.2",
    "vue-lazyload": "^1.3.2",
    "vue-plain-pagination": "^0.3.0"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.3",
    "ssh2shell": "^1.9.1"
  }
}